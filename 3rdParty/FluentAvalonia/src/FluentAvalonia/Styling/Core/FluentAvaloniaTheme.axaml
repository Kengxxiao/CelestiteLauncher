        <ResourceDictionary>
            <FontFamily x:Key="SymbolThemeFontFamily">/Fonts/#FluentSystemIcons-Resizable</FontFamily>
            <FontFamily x:Key="ContentControlThemeFontFamily">/Fonts/#MiSans</FontFamily>
